<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kirill.bskpayment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.kirill.bskpayment.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.kirill.bskpayment.Registration" />
        <activity android:name="com.example.kirill.bskpayment.MainActivity" />
        <activity android:name="com.example.kirill.bskpayment.UpdateBankCardList" />
        <activity android:name="com.example.kirill.bskpayment.UpdateMetropolitenCardList" />
        <activity android:name="com.example.kirill.bskpayment.Remittance" />
        <activity android:name="com.example.kirill.bskpayment.RemittanceStory"></activity>
    </application>

</manifest>